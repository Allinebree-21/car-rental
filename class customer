 public class Customer {
    private String name;
    private String id;

    public Customer(String name, String id) {
        this.name = name;
        this.id = id;
   }
 
 public void displayInfo() 
 {
 Scanner scanner = new Scanner(System.in);
  System.out.println("name: " + name);
  System.out.println("id: " + id);
 }

}
